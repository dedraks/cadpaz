{% for caso in casos %}
    {#{% render controller('CadpazBundle:Caso:view', {'id': caso.id}) %}#}
    <div class="telefone_endereco_secao">
        {% set num = caso.atendimentos|length %}
        <strong><a title="{{ caso.descricao }}" class="casoView" href="{{ path('caso_view', {'id': caso.id}) }}">{{ caso.nome }}</a></strong>{{ num == 0?" - Nenhum atendimento": " - " ~ num ~ " atendimento" }}{{ num>1?"s":"" }}<br>
    </div>
    <div class="spacer"></div>
{% endfor %}
<script>
    $(document).ready(function () {
        //alert('ready!');
        // This will automatically grab the 'title' attribute and replace
        // the regular browser tooltips for all <a> elements with a title attribute!
        $('.casoView').qtip();

    });
    
    $(".casoView").click(function(e) {
                        e.preventDefault();                        
                        $.ajax({
                            url: $(this).attr('href'),
                            method: 'POST',
                            //data: {'cpf': $("#cpf").val()},
                            success: function(result) {
                                window.listaDeCasos = $("#caso_area").html();
                                $("#caso_area").html(result);
                                //alert(result);
                            }
                        });
                    });
</script>