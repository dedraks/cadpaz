<script>
    
    {#//var c="{{create}}";#}


    {% if id is defined %}
        var url = Routing.generate('dedraks_config_encaminhamento_edit', {'id':{{id}}});
    {% else %}
        var url = Routing.generate('dedraks_config_encaminhamento_new');
    {% endif %}
    
    
    
    $('#newEncaminhamentoTipoForm').ajaxForm({
        url: url,
        type: 'post',
        success: function(responseText) {
            $("#dataArea").html(responseText);
            window.configPopup.close();
        }
    });
</script>